{
    "name": "ncsdess/conference-site",
    "description": "NC-SDESS 2026 Conference Website with Google Sheets and Email Integration",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "google/apiclient": "^2.15",
        "phpmailer/phpmailer": "^6.9"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "NcSdess\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
